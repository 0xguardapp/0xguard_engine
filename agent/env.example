# Unibase Configuration
UNIBASE_ACCOUNT=0x742d35Cc6634C0532925a3b844Bc9e8bE1595F0B
UNIBASE_RPC_URL=https://testnet.unibase.io
UNIBASE_CHAIN_ID=1337

# Membase Configuration
MEMBASE_ACCOUNT=default
MEMBASE_CONVERSATION_ID=bounty-audit-log
MEMBASE_ID=judge-agent

# Judge Agent Credentials
JUDGE_PRIVATE_KEY=your_private_key_here
BOUNTY_TOKEN_ADDRESS=0x...

# Target System (for testing)
# TARGET_SECRET_KEY is used as the secret seed for all agents (Judge, RedTeam, Target)
# Set this to: fetch_ai_agent_seed_or_random_hex (or any secure random hex string)
TARGET_SECRET_KEY=fetch_ai_agent_seed_or_random_hex
TARGET_API_URL=http://localhost:5000

# Monitoring & Logging
LOG_LEVEL=INFO
SENTRY_DSN=

# Agent Ports Configuration
# IMPORTANT: Port assignments to avoid conflicts
# These ports must match the configuration in config.py and docker-compose.yml
TARGET_PORT=8000      # Target Agent port
JUDGE_PORT=8002       # Judge Agent port
RED_TEAM_PORT=8001    # Red Team Agent port
AGENT_API_PORT=8003   # Agent API Server port (FastAPI server managing agent lifecycle)

# PHASE 3: Agent Registration Ports (optional overrides)
# If not set, will fall back to TARGET_PORT, JUDGE_PORT, RED_TEAM_PORT respectively
# AGENT_PORT_TARGET=8000
# AGENT_PORT_JUDGE=8002
# AGENT_PORT_RED_TEAM=8001

# Agent API Server Configuration
# URL for the agent API (used by frontend)
AGENT_API_URL=http://localhost:8003

# API Keys & External Services
ASI_API_KEY=your_asi_api_key_here
AGENTVERSE_KEY=your_agentverse_key_here
MAILBOX_KEY=your_mailbox_key_here
GEMINI_API_KEY=your_gemini_api_key_here  # Optional: Fallback LLM when ASI_API_KEY is missing

# Midnight Integration (Optional)
MIDNIGHT_API_URL=http://localhost:8100
MIDNIGHT_DEVNET_URL=http://localhost:6300
MIDNIGHT_BRIDGE_URL=http://localhost:3000
MIDNIGHT_CONTRACT_ADDRESS=
MIDNIGHT_SIMULATION_MODE=false

# Redis Configuration (for logging)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0

# Agent Registry & On-Chain Configuration
# Blockchain RPC for Optimism Sepolia (used by agent_registry_adapter)
OPTIMISM_SEPOLIA_RPC_URL=https://sepolia.optimism.io

# Private key for signing on-chain transactions (used by agent_registry_adapter)
# This should be a wallet private key with funds for gas fees
PRIVATE_KEY=your_private_key_here

# ERC-8004 Registry Contract Addresses (optional - for on-chain registration)
# If not set, registration will use Unibase storage only
IDENTITY_REGISTRY_ADDRESS=
REPUTATION_REGISTRY_ADDRESS=
VALIDATION_REGISTRY_ADDRESS=

